# When adding additional environment variables, the schema in "/src/env.js"
# should be updated accordingly.

# ============================================
# Server-side environment variables (sensitive data)
# If you want to use API key authentication (highest priority), set this
API_KEY=
# NextAuth secret for session encryption (required in production)
NEXTAUTH_SECRET=

# ============================================
# Client-side environment variables (public data)
# Log level: trace, debug, info, warn, error
NEXT_PUBLIC_LOG_LEVEL=debug

# API endpoints
NEXT_PUBLIC_API_URL=https://api.302.ai
NEXT_PUBLIC_AUTH_API_URL=https://dash-api.302.ai
NEXT_PUBLIC_AUTH_PATH=/auth

# Website URLs
NEXT_PUBLIC_302_WEBSITE_URL_CHINA=https://302ai.cn/
NEXT_PUBLIC_302_WEBSITE_URL_GLOBAL=https://302.ai/

# Configuration
NEXT_PUBLIC_DEFAULT_MODEL_NAME=gpt-4o-mini
NEXT_PUBLIC_HIDE_BRAND=true
NEXT_PUBLIC_IS_CHINA=false
NEXT_PUBLIC_DEFAULT_LOCALE=en

# Development configuration
# If you want to deploy on your own server or in production, keep it empty
NEXT_PUBLIC_DEV_HOST_NAME=
